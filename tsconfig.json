{
  "compilerOptions": {
    "incremental": true,
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@/*": ["*"],
      "@/components/*": ["components/*"],
      "@/slices/*": ["slices/*"],
      "@/utils/*": ["utils/*"],
      "@/types/*": ["types/*"],
      "@/data/*": ["data/*"],
      "@/styles/*": ["styles/*"],
      "@/svg/*": ["svg/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "./slicemachine/prismicio.d.ts"],
  "exclude": ["node_modules"]
}
